    <script>
        function toggleSidebar() {
            const sidebar = document.querySelector('.sidebar');
            const overlay = document.querySelector('.sidebar-overlay');
            sidebar.classList.toggle('active');
            overlay.classList.toggle('active');

            // Prevent body scroll when sidebar is open
            if (sidebar.classList.contains('active')) {
                document.body.style.overflow = 'hidden';
            } else {
                document.body.style.overflow = '';
            }
        }
    </script>

    <!-- Auth Script -->
    <script src="src/js/auth.js"></script>
    <script src="src/js/main.js"></script>
    <!-- Data Script -->
    <script src="src/js/data.js"></script>
    <script>
        // Check if user is logged in
        document.addEventListener('DOMContentLoaded', function () {
            if (!Auth.isAuthenticated()) {
                // Redirect to login if not authenticated
                window.location.href = 'login.html';
                return;
            }

            // Update user info in sidebar
            const user = Auth.getUser();
            if (user) {
                // Update avatar initials
                const avatarEl = document.querySelector('.user-avatar');
                if (avatarEl) {
                    avatarEl.textContent = Auth.getInitials();
                }

                // Update user name
                const nameEl = document.querySelector('.user-name');
                if (nameEl) {
                    nameEl.textContent = user.firstName + (user.lastName ? ' ' + user.lastName : '');
                }

                // Update email
                const emailEl = document.querySelector('.user-email');
                if (emailEl) {
                    emailEl.textContent = user.email;
                }

                // Update welcome message
                const welcomeEl = document.querySelector('.topbar-title');
                if (welcomeEl) {
                    welcomeEl.textContent = 'Welcome back, ' + user.firstName + '!';
                }
            }

            // Load dashboard data
            loadDashboardData();
        });

        // Load dynamic dashboard data
        function loadDashboardData() {
            // Get stats from UserData
            const stats = UserData.getDashboardStats();

            // Update stat cards
            document.getElementById('statTotalDonated').textContent = UserData.formatCurrency(stats.totalDonated);
            document.getElementById('statVolunteerHours').textContent = stats.volunteerHours;
            document.getElementById('statCausesSupported').textContent = stats.causesSupported;
            document.getElementById('statEventsAttended').textContent = stats.eventsRegistered;

            // Load recent activities
            loadActivities();

            // Load causes
            loadCauses();
        }

        // Load activities dynamically
        function loadActivities() {
            const activities = UserData.getActivities(5);
            const container = document.getElementById('activityContainer');

            if (activities.length === 0) {
                container.innerHTML = `
                    <div class="activity-item" style="justify-content: center; color: var(--text-muted);">
                        <p>No activities yet. Start by making a donation!</p>
                    </div>
                `;
                return;
            }

            container.innerHTML = activities.map(activity => {
                const iconClass = activity.type || 'donation';
                const iconSvg = getActivityIcon(activity.type);
                return `
                    <div class="activity-item">
                        <div class="activity-icon ${iconClass}">
                            ${iconSvg}
                        </div>
                        <div class="activity-content">
                            <h4>${activity.title}</h4>
                            <p>${UserData.formatDate(activity.date)}${activity.description ? ' � ' + activity.description : ''}</p>
                        </div>
                    </div>
                `;
            }).join('');
        }

        // Get activity icon based on type
        function getActivityIcon(type) {
            const icons = {
                donation: `<svg width="20" height="20" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1" />
                </svg>`,
                volunteer: `<svg width="20" height="20" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z" />
                </svg>`,
                event: `<svg width="20" height="20" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z" />
                </svg>`
            };
            return icons[type] || icons.donation;
        }

        // Load causes dynamically
        function loadCauses() {
            const causes = UserData.getCauses();
            const container = document.getElementById('causesContainer');

            if (!container) return;

            if (causes.length === 0) {
                container.innerHTML = `
                    <div style="text-align: center; padding: 2rem; color: var(--text-muted);">
                        <p>You haven't supported any causes yet.</p>
                        <a href="causes.html" class="btn btn-primary" style="margin-top: 1rem;">Explore Causes</a>
                    </div>
                `;
                return;
            }

            const causeImages = {
                'Education for All': 'https://images.unsplash.com/photo-1497633762265-9d179a990aa6?w=120&h=120&fit=crop',
                'Clean Water Initiative': 'https://images.unsplash.com/photo-1576765608535-5f04d1e3f289?w=120&h=120&fit=crop',
                'Healthcare Access': 'https://images.unsplash.com/photo-1584515933487-779824d29309?w=120&h=120&fit=crop',
                'General Fund': 'https://images.unsplash.com/photo-1469571486292-0ba58a3f068b?w=120&h=120&fit=crop'
            };

            container.innerHTML = causes.slice(0, 3).map(cause => {
                const img = causeImages[cause.name] || causeImages['General Fund'];
                const progress = Math.min(100, Math.round((cause.totalDonated / 50000) * 100));
                return `
                    <div class="cause-item">
                        <img src="${img}" alt="${cause.name}" class="cause-img">
                        <div class="cause-info" style="flex: 1;">
                            <h4>${cause.name}</h4>
                            <div style="font-size: 0.8125rem; color: var(--text-muted);">
                                ${UserData.formatCurrency(cause.totalDonated)} donated � ${cause.donationCount} donation${cause.donationCount > 1 ? 's' : ''}
                            </div>
                            <div class="cause-progress">
                                <div class="cause-progress-bar" style="width: ${progress}%;"></div>
                            </div>
                        </div>
                    </div>
                `;
            }).join('');
        }

        // Handle logout
        function handleLogout() {
            // Clear user data on logout (local cleanup)
            if (typeof UserData !== 'undefined') UserData.clearAllData();
            // Call global logout (includes redirect)
            if (typeof Auth !== 'undefined') Auth.logout();
            window.location.href = 'index.html';
        }

        // Function to add sample data for testing
        function addTestData() {
            UserData.addDonation({ amount: 5000, cause: 'Education for All' });
            UserData.addDonation({ amount: 2500, cause: 'Clean Water Initiative' });
            UserData.addVolunteerHours({ hours: 4, event: 'Teaching at Community Center' });
            UserData.registerEvent({ name: 'Annual Charity Run', date: '2026-02-15', location: 'Chennai Marina Beach' });
            loadDashboardData();
        }
    </script>
</body>

</html>


